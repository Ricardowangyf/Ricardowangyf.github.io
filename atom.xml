  <entry>
    <title>残酷天使的行动纲领</title>
    <link href="http://example.com/2023/02/26/%E6%AE%8B%E9%85%B7%E5%A4%A9%E4%BD%BF%E7%9A%84%E8%A1%8C%E5%8A%A8%E7%BA%B2%E9%A2%86/"/>
    <id>http://example.com/2023/02/26/%E6%AE%8B%E9%85%B7%E5%A4%A9%E4%BD%BF%E7%9A%84%E8%A1%8C%E5%8A%A8%E7%BA%B2%E9%A2%86/</id>
    <published>2023-02-26T00:00:00.000Z</published>
